{
  "routes": [
    {
      "path": "/",
      "page": "index",
      "routeType": "page",
      "layout": "main"
    },
    {
      "path": "/login",
      "page": "login",
      "layout": "securityLayout",
      "data": {
        "organisation_name": "INTEGEMS"
      }
    },
    {
      "path": "/request-password-reset",
      "page": "request-password-reset",
      "layout": "securityLayout"
    },
    {
      "path": "/reset-password",
      "page": "reset-password",
      "layout": "securityLayout"
    },
    {
      "path": "/dashboard",
      "page": "dashboard",
      "layout": "portal_layout"
    }
  ],
  "layouts": {
    "securityLayout": {
      "meta": [
        {
          "type": "text",
          "name": "organisation_name"
        }
      ],
      "exec": "/api/app_settings/security_app_data"
    },
    "portal_layout": {
      "exec": "/api/app_settings/security_app_data"
    }
  }
}