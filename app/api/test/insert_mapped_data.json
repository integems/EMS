{
  "meta": {
    "options": {
      "linkedFile": "/views/noise.ejs",
      "linkedForm": "add_noise_data"
    },
    "$_POST": [
      {
        "type": "file",
        "fieldName": "data_file",
        "options": {
          "rules": {
            "upload:accept": {
              "param": ".csv"
            }
          }
        },
        "name": "data_file",
        "sub": [
          {
            "name": "name",
            "type": "text"
          },
          {
            "name": "type",
            "type": "text"
          },
          {
            "name": "size",
            "type": "number"
          },
          {
            "name": "error",
            "type": "text"
          }
        ],
        "outputType": "file"
      },
      {
        "type": "text",
        "fieldName": "location_id",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "location_id"
      },
      {
        "type": "text",
        "name": "start_date_time"
      },
      {
        "type": "text",
        "name": "end_date_time"
      },
      {
        "type": "text",
        "name": "LAeq"
      },
      {
        "type": "text",
        "name": "LA90"
      },
      {
        "type": "text",
        "name": "LA10"
      },
      {
        "type": "text",
        "name": "LAFMax"
      },
      {
        "type": "text",
        "name": "LAFMin"
      }
    ]
  },
  "exec": {}
}