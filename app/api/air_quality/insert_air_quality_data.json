{
  "meta": {
    "options": {
      "linkedFile": "/views/air-quality.ejs",
      "linkedForm": "sc_add_air_data"
    },
    "$_POST": [
      {
        "type": "file",
        "fieldName": "data_file",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            },
            "upload:accept": {
              "param": ".csv"
            }
          }
        },
        "name": "data_file",
        "sub": [
          {
            "name": "name",
            "type": "text"
          },
          {
            "name": "type",
            "type": "text"
          },
          {
            "name": "size",
            "type": "number"
          },
          {
            "name": "error",
            "type": "text"
          }
        ],
        "outputType": "file"
      },
      {
        "type": "text",
        "fieldName": "location_id",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "location_id"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "record_repeat",
      "module": "core",
      "action": "repeat",
      "options": {},
      "meta": [
        {
          "name": "$index",
          "type": "number"
        },
        {
          "name": "$number",
          "type": "number"
        },
        {
          "name": "$name",
          "type": "text"
        },
        {
          "name": "$value",
          "type": "object"
        }
      ],
      "outputType": "array"
    }
  }
}